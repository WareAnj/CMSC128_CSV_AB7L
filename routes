POST    /faculty_user/register

POST    /faculty_user/post_volunteer
GET     /faculty_user/volunteers
PUT     /faculty_user/volunteer/:section_id/:student_number
DELETE  /faculty_user/volunteer/:section_id/:student_number

GET     /faculty_user/randomize
GET     /faculty_user/randomize_with_restriction        (if already called or not)

POST    /authenticate/login
GET     /authenticate/logout

GET     /admin/login_logs
GET     /admin/login_logs/:faculty_user_id

POST    /course/post_course
GET     /course/courses
PUT     /course/:faculty_user_id/:course_id
DELETE  /course/:faculty_user_id/:course_id
